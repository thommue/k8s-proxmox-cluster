---
apiVersion: kubeadm.k8s.io/v1beta3
kind: ClusterConfiguration
networking:
  podSubnet: {{ pod_subnet }}/16
  serviceSubnet: {{ service_subnet }}/16
---
apiVersion: kubeadm.k8s.io/v1beta3
kind: InitConfiguration
localAPIEndpoint:
  advertiseAddress: "{{ ip_address }}"
  bindPort: 6443
nodeRegistration:
  kubeletExtraArgs:
    node-ip: {{ ip_address }}